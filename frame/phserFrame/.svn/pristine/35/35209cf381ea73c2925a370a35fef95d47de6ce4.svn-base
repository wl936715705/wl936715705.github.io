    z-index: 10;
    width: 2.54rem;
    height: 0.24rem;
    background-size: 100% 100%;
}
.load_spot{
    position: absolute;
    top: 2.85rem;
    left: 1.04rem;
    z-index: 10;
    width: 0.47rem;
    height: 0.04rem;
    background-size: 100% 100%;
    overflow: hidden;
    clip: rect(0,0.47rem,0.04rem,0);
    -webkit-animation: load_spot 1.65s linear 0s infinite;
    animation: load_spot 1.65s linear 0s infinite;
}

