/**
 *动画
 */
@-webkit-keyframes load_ball {
    0%{
        -webkit-transform: rotate(0deg);
    }
    100%{
        -webkit-transform: rotate(360deg);
    }
}
@keyframes load_ball {
    0%{
        transform: rotate(0deg);
    }
    100%{
        transform: rotate(360deg);
    }
}

@-webkit-keyframes load_spot {
    0%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    5%{
        clip: rect(0,0.24rem,0.04rem,0);
    }
    30%{
        clip: rect(0,0.24rem,0.04rem,0);
    }
    40%{
        clip: rect(0,0.32rem,0.04rem,0);
    }
    50%{
        clip: rect(0,0.32rem,0.04rem,0);
    }
    60%{
        clip: rect(0,0.41rem,0.04rem,0);
    }
    70%{
        clip: rect(0,0.41rem,0.04rem,0);
    }
    80%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    90%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    100%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
}
@keyframes load_spot {
    0%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    5%{
        clip: rect(0,0.24rem,0.04rem,0);
    }
    30%{
        clip: rect(0,0.24rem,0.04rem,0);
    }
    40%{
        clip: rect(0,0.32rem,0.04rem,0);
    }
    50%{
        clip: rect(0,0.32rem,0.04rem,0);
    }
    60%{
        clip: rect(0,0.41rem,0.04rem,0);
    }
    70%{
        clip: rect(0,0.41rem,0.04rem,0);
    }
    80%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    90%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
    100%{
        clip: rect(0,0.47rem,0.04rem,0);
    }
}

@-webkit-keyframes light {
    0%{
        opacity: 0;
    }
    100%{
        opacity: 0.6;
    }
}
@keyframes light {
    0%{
        opacity: 0;
    }
    100%{
        opacity: 0.6;
    }
}

@-webkit-keyframes scale {
    0%{
        opacity: 0;
        -webkit-transform: scale(0.6,0.6);
    }
    50%{
        opacity: 1;
        -webkit-transform: scale(1.1,1.1);
    }
    100%{
        opacity: 1;
        -webkit-transform: scale(1,1);
    }
}
@keyframes scale {
    0%{
        opacity: 0;
        transform: scale(0.6,0.6);
    }
    50%{
        opacity: 1;
        transform: scale(1.1,1.1);
    }
    100%{
        opacity: 1;
        transform: scale(1,1);
    }
}