@charset "utf-8";
/*==================reset ===================*/
/*rem计算 width:750px-PSD*/
html{font-size: 100px;}
@media only screen and (max-width: 600px) {
    html{font-size: 80px;}
}
@media only screen and (max-width: 540px) {
    html{font-size: 72px;}
}
@media only screen and (max-width: 480px) {
    html{font-size: 64px;}
}
@media only screen and (max-width: 435px) {
    html{font-size: 58px;}
}
@media only screen and (max-width: 414px) {
    html{font-size: 55.2px;}
}
@media only screen and (max-width: 412px) {
    html{font-size: 54.93333333333333px;}
}
@media only screen and (max-width: 411px) {
    html{font-size: 54.8px;}
}
@media only screen and (max-width: 393px) {
    html{font-size: 52.4px;}
}
@media only screen and (max-width: 390px) {
    html{font-size: 52px;}
}
@media only screen and (max-width: 386px) {
    html{font-size: 51.46666666666667px;}
}
@media only screen and (max-width: 384px) {
    html{font-size: 51.2px;}
}
@media only screen and (max-width: 375px) {
    html{font-size: 50px;}
}
@media only screen and (max-width: 360px) {
    html{font-size: 48px;}
}
@media only screen and (max-width: 320px) {
    html{font-size: 42.66666666666667px;}
}

*{margin: 0;padding: 0;}
html, body {
    position: fixed;
    width: 100%;
    height: 100%;
    background: #fff;
    overflow: hidden;
}
ul, li {
    display: block;
    list-style: none;
}
a,a:hover{
    text-decoration: none;
}
em {
    font-style: normal;
}

button {
    font-family: Tahoma, Verdana, Arial;
    font-weight: normal;
}
a, button, input, img {
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
:focus {
    outline: 0;
}
.clearfix {
    zoom: 1
}
.clearfix:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
p,img,.show{
    display: block;
}
img[src=''] {
    display: none;
    opacity:0;
}
.hide {
    display: none;
}
.hidden {
    visibility: hidden;
}
/* ios设备平滑滚动 */
.smooth_slide {
    -webkit-overflow-scrolling: touch;
}
.img_respond{
    display: block;
    width: 100%;
    height: auto;
}

section{
    display: none;
    position: absolute;
    top:0;
    left:0;
    z-index: 10;
    width:100%;
    height:100%;
    overflow: hidden;
}

/**
 *landscape
 */
#landscape{
    background: url('../img/landscape.jpg') 50% 50%;
    background-size: cover;
    z-index: 9999;
}

#loading,#home,#last{
    bottom:0;
    height: 15rem;
    margin: auto;
}

#main{
    display: block;
}

/**
 *加载
 */
#loading{}
.load_box{
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 10;
    width: 2.54rem;
    height: 2.89rem;
    margin: auto;
}
.load_ball{
    position: absolute;
    top: 0;
    left: 0.56rem;
    z-index: 10;
    width: 1.36rem;
    height: 1.36rem;
    background-size: 100% 100%;
    -webkit-animation: load_ball 1.5s linear 0s infinite;
    animation: load_ball 1.5s linear 0s infinite;
}
.load_num{
    position: absolute;
    top: 1.62rem;
    left: 0;
    z-index: 10;
    width: 100%;
    height: 0.36rem;
    line-height: 0.36rem;
